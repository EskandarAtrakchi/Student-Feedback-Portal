<body>
  <header>
    <h1>Student Feedback Portal</h1>
    <nav>
      <a href="/">Home</a>
      <% if (currentUser) { %>
        | <span>Logged in as <%= currentUser.username %></span>
        | <a href="/logout">Logout</a>
      <% } else { %>
        | <a href="/register">Register</a>
        | <a href="/login">Login</a>
      <% } %>
    </nav>
    <hr />
  </header>

  <main>
    <%- body %>
  </main>
</body>
